{
	"name": "@datazod/shared",
	"version": "0.1.4",
	"private": false,
	"description": "Shared utilities for datazod packages",
	"author": {
		"name": "Tia-Lab",
		"url": "https://github.com/tia-lab"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/tia-lab/datazod.git",
		"directory": "packages/shared"
	},
	"homepage": "https://github.com/tia-lab/datazod/tree/main/packages/shared",
	"keywords": [
		"zod",
		"utilities",
		"typescript",
		"schema",
		"validation"
	],
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"index.ts"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"check-types": "tsc --noEmit",
		"lint": "echo \"Linting disabled for internal package\""
	},
	"peerDependencies": {
		"zod": "^3.x"
	},
	"devDependencies": {
		"zod": "^3.25.20"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT"
