	},
	"files": [
		"dist",
		"index.ts"
	],
	"scripts": {
		"build": "tsc",
		"dev": "bun src/index.ts",
		"test": "bun test",
		"lint": "echo \"Linting disabled for now\"",
		"check-types": "tsc --noEmit"
	},
	"peerDependencies": {
		"@libsql/client": "^0.x",
		"zod": "^3.x"
	},
	"dependencies": {
		"@datazod/shared": "^0.1.3"
	},
	"devDependencies": {
		"@libsql/client": "^0.5.1",
		"zod": "^3.25.20"
	},
	"publishConfig": {
		"access": "public",
		"dependencies": {
			"@datazod/shared": "^0.1.0"
		}
	},
	"license": "MIT"
